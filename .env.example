# Ollama Discord Bot Environment Variables
# Copy this file to .env and fill in your actual values

# Discord Bot Token
DISCORD_TOKEN=your_discord_bot_token_here

# Ollama Configuration (optional - can be overridden in YAML configs)
OLLAMA_BASE_URL=http://127.0.0.1:11434
OLLAMA_MODEL=llama3

# Development/Testing
DEBUG=false